{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory-Name",
			"defaultValue": "adf-aw-project-31032025"
		},
		"storagedatalake_accountKey": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"accountKey\" von \"storagedatalake\""
		},
		"httplinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"storagedatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://awstoragedatalak31032025.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI für Speicherkonto mit verknüpften ARM-Vorlagen"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "SAS-Token für Speicherkonto mit verknüpften ARM-Vorlagen"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-aw-project-31032025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storagedatalake_accountKey": {
						"value": "[parameters('storagedatalake_accountKey')]"
					},
					"httplinkedservice_properties_typeProperties_url": {
						"value": "[parameters('httplinkedservice_properties_typeProperties_url')]"
					},
					"storagedatalake_properties_typeProperties_url": {
						"value": "[parameters('storagedatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}